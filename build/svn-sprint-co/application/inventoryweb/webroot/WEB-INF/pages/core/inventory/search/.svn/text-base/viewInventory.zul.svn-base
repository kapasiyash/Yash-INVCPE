<zk>
	<window id=""  style="overflow: visible;" apply="com.elitecore.cpe.web.composer.inventory.search.ViewInventoryComposer">
		<hlayout height="100%" width="100%" style="overflow: visible;">
			 <vlayout width="100%" height="100%"   style="padding-left: 5px; padding-top: 2px;padding-bottom: 50px;">
		    	<hbox width="100%" style="border: 1px solid #ccc;border-width: 0 1px;">
		            <div align="right">
		                <menubar id="actionMenubar" autodrop="true" width="798px" sclass="top-menu"/>
		            </div>
		        </hbox>
				<hlayout width="100%">
					<vlayout width="100%">
						<grid id="viewbucketgrid" width="798px"  oddRowSclass="non-odd" sclass="viewGrid" zclass="none">
							<columns>
								<column width="150px" />
			                    <column width="249px" />
			                    <column width="150px" />
			                    <column width="249px" />
							</columns>
							<rows id="viewrows">
								<row spans="1, 3" align="top">
									<label value="${labels.inventory.Number}" style="text-align: left;" />
									<label style="text-align: left;" id="lbInventoryNumber"/>
								</row>
								<row spans="1, 3" align="top">
									<label value="${labels.inventory.batchNumber}" style="text-align: left;" />
									<label style="text-align: left;" id="lbBatchNumber"/>
								</row>
								<row spans="1, 3" align="top">
									<label value="${labels.inventory.Status}" style="text-align: left;" />
									<label style="text-align: left;" id="lbStatus"/>
								</row>
									<!-- Added By Rinkal Start-->								
									<row spans="1, 3" align="top">
									<label value="${labels.inventory.SubStatus}" style="text-align: left;" />
									<label style="text-align: left;" id="lbSubStatus"/>
								</row>
								<!-- Added By Rinkal End-->								
								<row spans="1, 3" align="top">
									<label value="${labels.warehouse.name}" style="text-align: left;" />
									<a><label style="text-align: left;" id="lbWarehouseName"/></a>
								</row>
								<row spans="1, 3" align="top">
									<label value="${labels.warehouse.transferStatusName}" style="text-align: left;" />
									<label style="text-align: left;" id="lbTransferStatusName"/>
								</row>
								<row spans="1, 3" align="top">
									<label value="${labels.warehouse.externalBatchNumber}" style="text-align: left;" />
									<label style="text-align: left;" id="lbExternalBatchNumber"/>
								</row>
								<row spans="1, 3" align="top">
									<label value="${labels.warehouse.guaranteewarrantymode}" style="text-align: left;" />
									<label style="text-align: left;" id="lbGuaranteeWarrantymode"/>
								</row>
								<row align="top">
									<label value="${labels.warehouse.warrantydate}" style="text-align: left;" />
									<label style="text-align: left;" id="lbWarrantydate"/>
									<label value="${labels.warehouse.warrantytype}" style="text-align: left;" />
									<label style="text-align: left;" id="lbWarrantytype"/>
								</row>
								<row align="top">
									<label value="${labels.item.resourceNo}" style="text-align: left;" />
									<a><label style="text-align: left;" id="lbResourceNumber"/></a>
									<label value="${labels.item.resourceName}" style="text-align: left;" />
									<label style="text-align: left;" id="lbResourceName"/>
								</row>
								<row align="top">
									<label value="${labels.item.type}" style="text-align: left;" />
									<label style="text-align: left;" id="lbResourceType"/>
									<label value="${labels.item.subtype}" style="text-align: left;" />
									<label style="text-align: left;" id="lbResourceSubType"/>
								</row>
<!-- 								<row align="top">
									<label value="${labels.inventory.acceptSS}" style="text-align: left;" />
									<label style="text-align: left;" id="lbAcceptedSubStatus"/>
									<label value="${labels.inventory.refurbishedSS}" style="text-align: left;" />
									<label style="text-align: left;" id="lbRefurbishedSubstatus"/>
								</row>
								<row align="top">
									<label value="${labels.inventory.standbySS}" style="text-align: left;" />
									<label style="text-align: left;" id="lbStandbySubStatus"/>
									<label value="${labels.inventory.newSS}" style="text-align: left;" />
									<label style="text-align: left;" id="lbNewSubstatus"/>
								</row>
 -->								<row align="top">
									<label value="${labels.global.createDate}" style="text-align: left;" />
									<label style="text-align: left;" id="lbCreatedDate"  />
									<label value="${labels.global.createDateBy}" style="text-align: left;" />
									<label style="text-align: left;" id="lbCreatedBy" />
								</row>
								<row align="top">
									<label value="${labels.global.lastModifiedDate}" style="text-align: left;" />
									<label style="text-align: left;" id="lbUpdateDate"  />
									<label value="${labels.global.lastModifiedBy}" style="text-align: left;" />
									<label style="text-align: left;" id="lbUpdatedBy" />
								</row>
							</rows>
							
						</grid>
						<div sclass="sectionTitle" width="800px">
							<label value="${labels.item.atributes}" style="font-weight: bold;" />
						</div>
						<grid  width="798px"  oddRowSclass="non-odd" sclass="viewGrid" zclass="none">
							<columns>
								<column width="150px" />
			                    <column width="249px" />
			                    <column width="150px" />
			                    <column width="249px" />
							</columns>
							<rows id="viewAttributerows">
							
							</rows>
						</grid>
						
						
