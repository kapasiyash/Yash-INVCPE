<!-- 			                    <column width="249px" /> -->
<!-- 							</columns> -->
<!-- 							<rows id="viewHistoryrows"> -->
								
<!-- 								</rows> -->
<!-- 						</grid> -->
					<separator bar="false" />
				
						<div id="viewActionContent"  width="100%" />
							<include  id="footer" width="99%"   src="/WEB-INF/pages/core/common/Footer.zul" />
						</vlayout>
				</hlayout>
			</vlayout>
		</hlayout>
	</window>

</zk>