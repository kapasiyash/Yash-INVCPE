<?page title="" contentType="text/html;charset=UTF-8"?>
<zk>
	<window id="History" border="normal" title="Agent Run Detail" width="860px" height="400px" 
	position="center,center" closable="true" style="overflow:auto;" apply="com.elitecore.cpe.web.system.agent.ViewHistoryPopupComposer">
		<tabbox width="100%" height="100%" tabscroll="false" id="createLPTabbox">
			<tabs>
				<tab id="createLPTab" label="${labels.vagenthistory.abtitle}" />
			</tabs>
			<tabpanels height="100%" width="100%">
				<tabpanel style="padding-top: 10px; padding-left: 20px;overflow:auto;" sclass="main-cont">
		<hlayout height="100%" width="100%" style="overflow: visible;">
			 <vlayout width="796px" height="100%" style="padding-top: 5px; padding-left: 10px; padding-right: 10px;">
			 <separator bar="false"/>
		    	<div sclass="sectionTitle">
				<label value="${labels.vagenthistory.abtitle}"
					style="font-weight: bold;" />
			</div>
			<separator bar="false"/>
				<hlayout width="100%">
					<vlayout width="800px">
						<grid id="viewagenthistorygrid" width="793px"  oddRowSclass="non-odd" sclass="viewGrid" zclass="none">
							<columns>
								<column width="200px" />
			                    <column width="250px" />
			                    <column width="150px" />
			                    <column width="193px" />
							</columns>
							<rows>
							
								<row align="top">
									<label value="${labels.vagenthistory.startdate}" style="text-align: left;" />
									<label style="text-align: left;" id="lbstartdate"/>
									<label value="${labels.vagenthistory.stopdate}" style="text-align: left;" />
									<label style="text-align: left;" id="lbstopdate" />
								</row>
								
								<row spans="1, 3" align="top">
									<label value="${labels.vagenthistory.remarks}" style="text-align: left;" />
									<label style="text-align: left;" id="lbRemarks" />
								</row>
								<row align="top">
									<label value="${labels.vagenthistory.master}" style="text-align: left;" />
									<label style="text-align: left;" id="lbMaster"  />
									<label value="${labels.vagenthistory.entity}" style="text-align: left;" />
									<label style="text-align: left;" id="lbEntity" />
								</row>
								<row align="top">
									<label value="${labels.vagenthistory.mastersucceed}" style="text-align: left;" />
									<label style="text-align: left;" id="lbMasterSucced"  />
									<label value="${labels.vagenthistory.entitysucceed}" style="text-align: left;" />
									<label style="text-align: left;" id="lbEntitySucced" />
								</row>
								<row spans="1, 3" align="top">
									<label value="${labels.vagenthistory.status}" style="text-align: left;" />
									<label style="text-align: left;" id="lbStatus"  />
									
								</row>
								
							</rows>
						</grid>
						<separator bar="false"/>
						
						 <!-- <hlayout style="text-align: left;">
           					 <button label="${labels.global.okBtn}" mold="trendy" id="btnOk" width="80px"/>
       					 </hlayout> -->
						
						<separator bar="false"/>
						
						
						</vlayout>
				</hlayout>
			</vlayout>
		</hlayout>
			</tabpanel>
			</tabpanels>
		</tabbox>
	</window>
</zk>